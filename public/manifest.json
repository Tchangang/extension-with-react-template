{
    "manifest_version": 2,
    "name": "ReactXion",
    "description": "Chrome Extension template with React",
    "version": "0.1.1",
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Open popup"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_security_policy": "script-src 'self' 'sha256-DZDGRim4uw5o4+ai7rL0kErvDKkn3PiOo6EWd/IET4M=' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "storage"
    ]
}